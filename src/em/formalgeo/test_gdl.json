      "ac_check": "Ueq(Sub(a.k,b.k))&Ueq(Sub(l.k,a.k))&Ueq(Sub(l.k,b.k))&G(Sub(MAM(a.k,b.k),MAM(a.k,l.k)))",
      "premise": "PointOnLine(O,l)&PointOnLine(O,a)&PointOnLine(O,b)&Eq(Sub(al.ma,lb.ma))",
      "conclusion": [
        "AngleBisector(O,l,a,b)"
      ],
      "proving": {}
    },
