{
  "Notes": [
    "1.algebraic_forms 中 只能出现 实体相关的属性",
    "2.Theorem定义 中 只能出现 关系相关的属性",
    "3.构图中 只能出现 关系相关的属性",
    "4.定理分为两类，判定定理determination和性质定理property"
  ],
  "Attributions": {
    "XOfPoint(A)": {
      "geometric_constraints": "Point(A)",
      "algebraic_forms": "A.x",
      "sym": "x",
      "multiple_forms": "XOfPoint(A)"
    },
    "YOfPoint(A)": {
      "geometric_constraints": "Point(A)",
      "algebraic_forms": "A.y",
      "sym": "y",
      "multiple_forms": "YOfPoint(A)"
    },
    "KOfLine(l)": {
      "geometric_constraints": "Line(l)",
      "algebraic_forms": "l.k",
      "sym": "k",
      "multiple_forms": "KOfLine(l)"
    },
    "BOfLine(l)": {
      "geometric_constraints": "Line(l)",
      "algebraic_forms": "l.b",
      "sym": "b",
      "multiple_forms": "BOfLine(l)"
    },
    "UOfCircle(O)": {
      "geometric_constraints": "Circle(O)",
      "algebraic_forms": "O.u",
      "sym": "u",
      "multiple_forms": "UOfCircle(O)"
    },
    "VOfCircle(O)": {
      "geometric_constraints": "Circle(O)",
      "algebraic_forms": "O.v",
      "sym": "v",
      "multiple_forms": "VOfCircle(O)"
    },
    "ROfCircle(O)": {
      "geometric_constraints": "Circle(O)",
      "algebraic_forms": "O.r",
      "sym": "r",
      "multiple_forms": "ROfCircle(O)"
    },
    "DistanceBetweenPointAndPoint(A,B)": {
      "geometric_constraints": "Point(A)&Point(B)",
      "algebraic_forms": "SquaredNorm(A.x,B.x,A.y,B.y)",
      "sym": "dpp",
      "multiple_forms": "DistanceBetweenPointAndPoint(A,B)|DistanceBetweenPointAndPoint(B,A)"
    },
    "DistancePowBetweenPointAndLine(A,l)": {
      "geometric_constraints": "Point(A)&Line(l)",
      "algebraic_forms": "Div(Pow(Add(Sub(Mul(l.k,A.x),A.y),l.b),2),Add(Pow(l.k,2),1))",
      "sym": "dpl",
      "multiple_forms": "DistancePowBetweenPointAndLine(A,l)"
    },
    "DistanceBetweenPointAndCircle(A,O)": {
      "geometric_constraints": "Point(A)&Circle(O)",
      "algebraic_forms": "SquaredNorm(A.x,O.u,A.y,O.v)",
      "sym": "dpc",
      "multiple_forms": "DistanceBetweenPointAndCircle(A,O)"
    },
    "MeasureOfAngle(l,k)": {
      "geometric_constraints": "Line(l)&Line(k)",
      "algebraic_forms": "Ma(l.k,k.k)",
      "sym": "ma",
      "multiple_forms": "MeasureOfAngle(l,k)"
    },
    "RadiusOfCircle(O)": {
      "geometric_constraints": "Circle(O)",
      "algebraic_forms": "O.r",
      "sym": "rc",
      "multiple_forms": "RadiusOfCircle(O)"
    }
  },
  "Relations": {
    "FreePoint(A)": {
      "geometric_constraints": "Point(A)",
      "algebraic_forms": ""
    },
    "FreeLine(l)": {
      "geometric_constraints": "Line(l)",
      "algebraic_forms": ""
    },
    "FreeCircle(O)": {
      "geometric_constraints": "Circle(O)",
      "algebraic_forms": "G(O.r)"
    },
    "SamePoint(A,B)": {
      "geometric_constraints": "Point(A)&Point(B)",
      "algebraic_forms": "Eq(SquaredNorm(A.x,B.x,A.y,B.y))"
    },
    "SameLine(l,m)": {
      "geometric_constraints": "Line(l)&Line(m)",
      "algebraic_forms": "Eq(SquaredNorm(l.k,m.k,l.b,m.b))"
    },
    "SameCircle(O,P)": {
      "geometric_constraints": "Circle(O)&Circle(P)",
      "algebraic_forms": "Eq(SquaredNorm(O.u,P.u,O.v,P.v))&Eq(Sub(O.r,P.r))&G(O.r)&G(P.r)"
    },
    "PointOnLine(A,l)": {
      "geometric_constraints": "Point(A)&Line(l)",
      "algebraic_forms": "Eq(Sub(A.y,Add(Mul(l.k,A.x),l.b)))"
    },
    "PointOnCircle(A,O)": {
      "geometric_constraints": "Point(A)&Circle(O)",
      "algebraic_forms": "Eq(Pp(A.x,A.y,O.u,O.v,O.r))&G(O.r)"
    },
    "PointLeftSegment(P,A,B)": {
      "geometric_constraints": "Point(P)&Point(A)&Point(B)",
      "algebraic_forms": "G(Sub(Mul(Sub(B.x,A.x),Sub(P.y,A.y)),Mul(Sub(B.y,A.y),Sub(P.x,A.x))))"
    },
    "ParallelBetweenLine(l,m)": {
      "geometric_constraints": "Line(l)&Line(m)",
      "algebraic_forms": "Eq(Sub(l.k,m.k))&Ueq(Sub(l.b,m.b))"
    },
    "EqualDistance(A,B,C,D)": {
      "geometric_constraints": "Point(A)&Point(B)&Point(C)&Point(D)",
      "algebraic_forms": "Eq(Sub(SquaredNorm(A.x,B.x,A.y,B.y),SquaredNorm(C.x,D.x,C.y,D.y)))&Ueq(SquaredNorm(A.x,B.x,A.y,B.y))"
    },
    "EqualAngle(l,m,x,y)": {
      "geometric_constraints": "Line(l)&Line(m)&Line(x)&Line(y)",
      "algebraic_forms": "Eq(Sub(Ma(l.k,m.k),Ma(x.k,y.k)))&Ueq(SquaredNorm(l.k,m.k,l.b,m.b))&Ueq(SquaredNorm(x.k,y.k,x.b,y.b))&Ueq(SquaredNorm(l.k,x.k,l.b,x.b,m.k,y.k,m.b,y.b))"
    },
    "AngleBisector(O,l,a,b)": {
      "format_constraints": "NotSameLetter(a,b)",
      "geometric_constraints": "Point(O)&Line(l)&Line(a)&Line(b)",
      "algebraic_forms": "PointOnLine(O,l)&PointOnLine(O,a)&PointOnLine(O,b)&Ueq(SquaredNorm(a.k,b.k,a.b,b.b))&EqualAngle(a,l,l,b)&Leq(Sub(Add(Ma(a.k,l.k),Ma(l.k,b.k)),180))"
    },
    "MidpointOfArc(M,O,A,B)": {
      "geometric_constraints": "Point(M)&Circle(O)&Point(A)&Point(B)",
      "algebraic_forms": "PointOnCircle(M,O)&PointOnCircle(A,O)&PointOnCircle(B,O)&EqualDistance(A,M,M,B)&PointLeftSegment(M,B,A)"
    },
    "Triangle(A,c,B,a,C,b)": {
      "geometric_constraints": "Point(A)&Line(c)&Point(B)&Line(a)&Point(C)&Line(b)",
      "algebraic_forms": "PointOnLine(A,c)&PointOnLine(B,c)&PointOnLine(B,a)&PointOnLine(C,a)&PointOnLine(C,b)&PointOnLine(A,b)&PointLeftSegment(C,A,B)"
    },
    "CongruentTriangle(A,c,B,a,C,b,D,f,E,d,F,e)": {
      "geometric_constraints": "Point(A)&Line(c)&Point(B)&Line(a)&Point(C)&Line(b)&Point(D)&Line(f)&Point(E)&Line(d)&Point(F)&Line(e)",
      "algebraic_forms": "Triangle(A,c,B,a,C,b)&Triangle(D,f,E,d,F,e)&EqualDistance(A,B,D,E)&EqualDistance(B,C,E,F)&EqualDistance(C,A,F,D)&Ueq(SquaredNorm(A.x,D.x,A.y,D.y,B.x,E.x,B.y,E.y,C.x,F.x,C.y,F.y))"
    }
  },
  "Theorems": {
    "triangle_determination(A,c,B,a,C,b)": {
      "premises": "PointOnLine(A,c)&PointOnLine(B,c)&PointOnLine(B,a)&PointOnLine(C,a)&PointOnLine(C,b)&PointOnLine(A,b)",
      "conclusion": "Triangle(A,c,B,a,C,b)"
    },
    "parallel_property_algebraic(l,m)": {
      "premises": "ParallelBetweenLine(l,m)",
      "conclusion": "Eq(lm.ma)"
    },
    "angle_bisector_property_distance_equal(O,A,l,a,b)": {
      "premises": "AngleBisector(O,l,a,b)&PointOnLine(A,l)&~SamePoint(O,A)",
      "conclusion": "Eq(Sub(Aa.dpl,Ab.dpl))"
    },
    "parallel_multiple_forms(l,m)": {
      "premises": "ParallelBetweenLine(l,m)",
      "conclusion": "ParallelBetweenLine(m,l)"
    },
    "parallel_property(l,m,n)": {
      "premises": "ParallelBetweenLine(l,m)&ParallelBetweenLine(m,n)",
      "conclusion": "ParallelBetweenLine(l,n)"
    },
    "parallel_property_angle_equal(l,m,a)": {
      "premises": "ParallelBetweenLine(l,m)&Line(a)",
      "conclusion": "Eq(Sub(la.ma,ma.ma))"
    },
    "congruent_triangle_determination_asa(A,c,B,a,C,b,D,f,E,d,F,e)": {
      "premises": "Triangle(A,c,B,a,C,b)&Triangle(D,f,E,d,F,e)&Eq(Sub(ca.ma,fd.ma))&Eq(Sub(ab.ma,de.ma))&Eq(Sub(BC.dpp,EF.dpp))",
      "conclusion": "CongruentTriangle(A,c,B,a,C,b,D,f,E,d,F,e)"
    },
    "congruent_triangle_property_multiple_forms(A,c,B,a,C,b,D,f,E,d,F,e)": {
      "premises": "CongruentTriangle(A,c,B,a,C,b,D,f,E,d,F,e)",
      "conclusion": "CongruentTriangle(B,a,C,b,A,c,E,d,F,e,D,f)"
    },
    "congruent_triangle_property_line_equal(A,c,B,a,C,b,D,f,E,d,F,e)": {
      "premises": "CongruentTriangle(A,c,B,a,C,b,D,f,E,d,F,e)",
      "conclusion": "Eq(Sub(AB.dpp,DE.dpp))"
    },
    "angle_bisector_property_equal_angle(O,l,a,b)": {
      "premises": "AngleBisector(O,l,a,b)",
      "conclusion": "Eq(Sub(al.ma,lb.ma))"
    },
    "angle_bisector_determination_angle_equal(O,l,a,b)": {
      "premises": "PointOnLine(O,l)&PointOnLine(O,a)&PointOnLine(O,b)&Eq(Sub(al.ma,lb.ma))",
      "conclusion": "AngleBisector(O,l,a,b)"
    },
    "angle_bisector_determination_distance_equal(O,A,l,a,b)": {
      "premises": "PointOnLine(O,l)&PointOnLine(O,a)&PointOnLine(O,b)&Eq(Sub(Aa.dpl,Ab.dpl))&~SamePoint(O,A)",
      "conclusion": "AngleBisector(O,l,a,b)"
    },
    "line_property_adjacent_complementary_angle(l,k)": {
      "premises": "Line(l)&Line(k)",
      "conclusion": "Eq(Sub(Add(lk.ma,kl.ma),180))"
    }
  }
}